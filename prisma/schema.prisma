datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

generator client {
  provider = "prisma-client-js"
}

model Vehicle {
  id    Int    @id @default(autoincrement())
  bicycle String
  color String
  fuel String
  manufacturer String
  type String
  vehicle String
  vin String
  vrm String

  @@map("vehicle")
}